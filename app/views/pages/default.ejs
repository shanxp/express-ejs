<main>
    <div class="jumbotron">
        <h1>This is great <%= __('Hello') %> <%=config.app.name %> <%=config.app.version %></h1>
        <h3><%= __n('%s dog', 50) %></h3>
        <p>Welcome to templating using EJS</p>
        <% if(params.users && params.users.length) { %>
        <ul>
        <% 
            for (let i=0; i<params.users.length; i++) {
                let user = params.users[i];
        %>
        <li> <%=user.firstname %> </li>
        <% } %>
        </ul>
        <% } else { %>
        <b> No users! </b>
        <% } %>
    </div>
</main>